# Copy this file to terraform.tfvars and fill in your values

aws_region    = "us-east-1"
environment   = "production"
project_name  = "cinematch"

# Domain Configuration
domain_name = "api.cinematch.muhacodes.com"

# Database Configuration
db_password = "your-strong-password-here"  # Change this!

# EC2 Configuration
ec2_key_name = "your-ec2-key-pair-name"  # Create this in AWS Console first

# DockerHub
dockerhub_image = "yourusername/cinematch:latest"  # Your DockerHub image

# API Keys (Keep these secret!)
tmdb_token        = "your-tmdb-read-access-token"
llm_api_key       = "your-llm-api-key"
django_secret_key = "your-django-secret-key"  # Generate with: python -c 'from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())'

